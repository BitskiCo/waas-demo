{
  "name": "waas-demo",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev --host=127.0.0.1",
    "build": "cross-env CF_PAGES=1 vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "schema": "rover supergraph fetch Bitski@current > schema.graphql"
  },
  "devDependencies": {
    "@apollo/rover": "^0.10.0",
    "@ethersproject/providers": "^5.7.2",
    "@panva/oauth4webapi": "^1.2.1",
    "@sveltejs/adapter-cloudflare-workers": "^1.1.2",
    "@sveltejs/kit": "^1.20.2",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "autoprefixer": "^10.4.13",
    "bitski-provider": "^2.1.0-next.2",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.13",
    "dotenv": "^16.1.4",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "eth-provider-types": "^0.2.0-next.2",
    "postcss": "^8.4.17",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.8.1",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "svelte": "^3.46.0",
    "svelte-check": "^2.7.1",
    "svelte-preprocess": "^4.10.6",
    "tailwindcss": "^3.1.8",
    "tslib": "^2.3.1",
    "typescript": "^4.7.4",
    "util": "^0.12.5",
    "vite": "^4.0.0",
    "vite-plugin-node-polyfills": "^0.9.0"
  },
  "type": "module"
}
